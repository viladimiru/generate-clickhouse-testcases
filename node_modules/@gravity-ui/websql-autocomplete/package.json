  "scripts": {
    "build": "rimraf dist && node build.js && tsc -p tsconfig.build.json --declarationDir dist/types",
    "fix": "run-p fix:*",
    "fix:antlr": "antlr-format --config antlr-formatter.config.json src/autocomplete/databases/**/grammar/*.g4",
